		"clsx": "^1.2.1",
		"cookies-next": "^2.1.1",
		"dotenv": "16.0.0",
		"jsonwebtoken": "^8.5.1",
		"marked": "^4.2.2",
		"next": "13.0.3-canary.4",
		"next-auth": "^4.16.4",
		"next-joi": "^2.2.1",
		"next-themes": "npm:@wits/next-themes@0.2.7",
		"prism-react-renderer": "^1.3.5",
		"rc-table": "7.24.1",
		"react": "18.2.0",
		"react-datepicker": "4.8.0",
